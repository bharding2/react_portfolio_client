{
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "sourceType": "module"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-unused-vars": 0,
    "prefer-arrow-callback": 0
  }
}
